  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listing publish api - Node attach",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
  },
  {
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "program": "${workspaceFolder}\\listing-publish-api\\server.js"
  }]
}